{
  "deno.enable": true,
  "deno.lint": true,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "cSpell.words": [
    "preact",
    "uncompiled"
  ],
  "emmet.includeLanguages": {
    "javascriptreact": "html",
    "typescriptreact": "html"
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  }
}
